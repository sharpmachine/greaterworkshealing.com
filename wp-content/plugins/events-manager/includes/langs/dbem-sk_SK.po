#: admin/em-options.php:590 admin/em-options.php:745 admin/em-options.php:835
#, php-format
msgid "Default %s list options"
msgstr "Predvolené možnosti zoznamu %s"

# @ dbem
#: admin/em-options.php:591 admin/em-options.php:746 admin/em-options.php:836
msgid "These can be overriden when using shortcode or template tags."
msgstr "Toto môže byť potlačený pri použití krátky alebo šablóna Tagy."

# @ dbem
#: admin/em-options.php:595 admin/em-options.php:1103
msgid "Default event list ordering"
msgstr "Predvolená akcia zoznam objednanie"

# @ dbem
